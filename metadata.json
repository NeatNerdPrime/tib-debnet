{
  "name": "trepasi-debnet",
  "version": "1.0.0",
  "author": "T. Repasi",
  "summary": "Puppet module to manage /etc/network/interface file on Debian based Linux systems.",
  "tags": ["network","interfaces","ifup","ifdown","bridge"],
  "license": "Apache-2.0",
  "project_page": "https://github.com/rtib/tib-debnet.git",
  "source": "https://github.com/rtib/tib-debnet.git",
  "issues_url":"https://github.com/rtib/tib-debnet/issues",
  "operatingsystem_support": [
    {"operatingsystem": "Debian", "operatingsystemrelease": ["7"] },
    {"operatingsystem": "Ubuntu", "operatingsystemrelease": ["14.04"] }
